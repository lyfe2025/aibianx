{
  "kind": "collectionType",
  "collectionName": "subscriptions",
  "info": {
    "singularName": "subscription",
    "pluralName": "subscriptions",
    "displayName": "会员订阅",
    "description": "会员订阅系统 - 管理用户会员服务状态和权限"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "required": true,
      "description": "订阅用户"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "description": "关联订单"
    },
    "planType": {
      "type": "enumeration",
      "enum": ["monthly", "yearly", "lifetime"],
      "required": true,
      "description": "订阅类型：月付/年付/终身"
    },
    "planName": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "description": "套餐名称"
    },
    "startDate": {
      "type": "date",
      "required": true,
      "description": "开始日期"
    },
    "endDate": {
      "type": "date",
      "description": "结束日期"
    },
    "autoRenew": {
      "type": "boolean",
      "default": false,
      "description": "自动续费"
    },
    "status": {
      "type": "enumeration",
      "enum": ["active", "expired", "cancelled", "suspended"],
      "default": "active",
      "description": "订阅状态"
    },
    "features": {
      "type": "json",
      "description": "订阅特权列表"
    }
  }
}